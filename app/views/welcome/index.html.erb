<%= flash[:notice] %>


<%= form_tag "/checkout" do %>
  <%= label_tag(:price, "price") %>
  <%= text_field_tag(:price) %>
  <%= label_tag(:quantity, "quantity") %>
  <%= text_field_tag(:quantity) %>
  <%= submit_tag("checkout") %>
<% end %>


<h2>transactions</h2>
<%= @transactions.each do |t| %>
  <%= t.out_trade_no %>
  <br>
<% end %>
